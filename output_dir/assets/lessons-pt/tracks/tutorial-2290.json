            "interactiveContent": "<!DOCTYPE html ><html><body><p>Codifique uma instrução <code>UPDATE</code> para aumentar a contagem de <code>streams</code> de Paradise para 130.</p><database visible-if=\"always\" bootstrapScript=\"CREATE TABLE Songs (&#xA; songname TEXT NOT NULL,&#xA; artist TEXT NOT NULL,&#xA; streams INTEGER NOT NULL,&#xA; certifications TEXT&#xA;);&#xA;&#xA;INSERT INTO Songs (songname, artist, streams) VALUES ('Paradise', 'The Jets', 120);&#xA;&#xA;INSERT INTO Songs (songname, artist, streams) VALUES ('My Way', 'Stevie J', 100);&#xA;&#xA;INSERT INTO Songs (songname, artist, streams) VALUES ('Purple Sunrise', 'Harry', 90);&#xA;&#xA;INSERT INTO Songs (songname, artist, streams) VALUES ('California Highway', 'Joan &amp; Jerry', 85);&#xA;&#xA;INSERT INTO Songs (songname, artist, streams) VALUES ('My Sister Meg', 'Couches', 70);\"><tables><table name=\"Songs\"><row><header content=\"songname\" /><header content=\"artist\" /><header content=\"streams\" /><header content=\"certifications\" /></row><row><data content=\"Paradise\" /><data content=\"The Jets\" /><data content=\"120\" /><data content=\"\" /></row><row><data content=\"My Way\" /><data content=\"Stevie J\" /><data content=\"100\" /><data content=\"\" /></row><row><data content=\"Purple Sunrise\" /><data content=\"Harry\" /><data content=\"90\" /><data content=\"\" /></row><row><data content=\"California Highway\" /><data content=\"Joan &amp; Jerry\" /><data content=\"85\" /><data content=\"\" /></row><row><data content=\"My Sister Meg\" /><data content=\"Couches\" /><data content=\"70\" /><data content=\"\" /></row></table></tables></database><code version=\"2\" codelang=\"sql\" name=\"script.sql\" visible-if=\"always\"><content><text value=\"UPDATE\" class=\"keyword\" /><text value=\" Songs \" /><text value=\"SET\" class=\"keyword\" /><text value=\" streams = \" /><text value=\"130\" class=\"number\" /><text value=\" \" /><text value=\"WHERE\" class=\"keyword\" /><text value=\" songname =