              "hasVisualOutput": false,
              "files": [
                {
                  "name": "script.sql",
                  "codeLanguage": "sql",
                  "content": "SELECT *\r\nFROM artist\r\n;",
                  "solvedContent": "SELECT *\r\nFROM artist\r\nINNER JOIN artwork\r\nON artist.name = artwork.artist_name;"
