            "contentType": "interactive",
            "images": [],
            "interactiveContent": "<!DOCTYPE html ><html><body><p>Code a condition to filter for items where the numeric <code>year</code> property equals <code>2011</code>.</p><database visible-if=\"always\" bootstrapScript=\"CREATE TABLE books (&#xA; title varchar(255),&#xA; year int,&#xA; genre varchar(255)&#xA;);&#xA;&#xA;INSERT INTO books(title, year, genre)&#xA;VALUES ('Ulysses', 1992, 'fiction');&#xA;&#xA;INSERT INTO books(title, year, genre)&#xA;VALUES ('Sapiens', 2011, 'non-fiction');&#xA;&#xA;INSERT INTO books(title, year, genre)&#xA;VALUES ('Beloved', 1987, 'fiction');&#xA;&#xA;INSERT INTO books(title, year, genre)&#xA;VALUES ('The Prince', 1532, 'non-fiction');\"><tables><table name=\"books\"><row><header content=\"title\" /><header content=\"year\" /><header content=\"genre\" /></row><row><data content=\"Ulysses\" /><data content=\"1992\" /><data content=\"fiction\" /></row><row><data content=\"Sapiens\" /><data content=\"2011\" /><data content=\"non-fiction\" /></row><row><data content=\"Beloved\" /><data content=\"1987\" /><data content=\"fiction\" /></row><row><data content=\"The Prince\" /><data content=\"1532\" /><data content=\"non-fiction\" /></row></table></tables></database><code version=\"2\" codelang=\"sql\" name=\"script.sql\" visible-if=\"always\"><content><text value=\"SELECT\" class=\"keyword\" /><text value=\" *\" /><text type=\"break\" /><text value=\"FROM\" class=\"keyword\" /><text value=\" books\" /><text type=\"break\" /><text value=\"WHERE\" class=\"keyword\" /><text value=\" \" /><text value=\"year\" class=\"keyword\" /><text value=\" = \" /><text value=\"2011\" class=\"number\" /><text value=\";\" /></content><interactions><interaction type=\"validatedInput\" isDefault=\"true\" startIndex=\"26\" endIndex=\"37\" /></interactions><output><table name=\"Result\"><row><header content=\"title\" /><header content=\"year\" /><header content=\"genre\" /></row><row><data content=\"Sapiens\" /><data content=\"2011\" /><data content=\"non-fiction\" /></row></table></output></code><p visible-if=\"wrong\">Make sure to type <code>year = 2011</code>.</p></body></html>"
