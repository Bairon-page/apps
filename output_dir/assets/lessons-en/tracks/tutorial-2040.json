            "id": 117437,
            "contentType": "interactive",
            "images": [],
            "interactiveContent": "<!DOCTYPE html ><html><body><p>What's the purpose of WAI-ARIA?<br /></p><selection iscode=\"false\" multiple=\"multiple\"><item value=\"1\" correct=\"true\">To ensure websites meet accessibility standards</item><item value=\"2\" correct=\"false\">To use semantic tags</item></selection><p visible-if=\"wrong\">To ensure websites meet accessibility standards.</p></body></html>"
          },
          {
