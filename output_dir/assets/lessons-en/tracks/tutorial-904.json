            "contentType": "interactive",
            "images": [],
            "interactiveContent": "<!DOCTYPE html ><html><body><p>Time to test what you've learned so far! What type of tag is this? <code>&lt;/button&gt;</code> .</p><selection iscode=\"false\" multiple=\"multiple\"><item value=\"1\" correct=\"true\">A closing tag</item><item value=\"2\" correct=\"false\">An opening tag</item></selection><p visible-if=\"wrong\">A closing tag.</p></body></html>"
