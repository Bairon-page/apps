            "images": [],
            "interactiveContent": "<!DOCTYPE html ><html><body><p>What do we use tree data structures for?</p><selection iscode=\"false\" multiple=\"multiple\"><item value=\"1\" correct=\"true\">To represent data and relationships in a hierarchical way</item><item value=\"2\" correct=\"false\">To represent data in a linear way</item></selection><p visible-if=\"wrong\">To represent data and relationships in a hierarchical way.</p></body></html>"
          },
