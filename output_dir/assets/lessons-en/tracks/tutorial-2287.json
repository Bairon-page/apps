            "contentType": "interactive",
            "images": [],
            "interactiveContent": "<!DOCTYPE html ><html><body><p>Select the newest game's <code>name</code> under an alias, with <code>SELECT name</code>, <code>AS</code> and the alias <code>newest</code>.</p><database visible-if=\"always\" bootstrapScript=\"CREATE TABLE mario_games (&#xA; name varchar(255),&#xA; release int&#xA;);&#xA;&#xA;INSERT INTO mario_games (name, release)&#xA;VALUES ('Mario Bros.', 1983);&#xA;&#xA;INSERT INTO mario_games (name, release)&#xA;VALUES ('Mario Clash', 1995);&#xA;&#xA;INSERT INTO mario_games (name, release)&#xA;VALUES ('Super Mario Galaxy', 2007);&#xA;&#xA;INSERT INTO mario_games (name, release)&#xA;VALUES ('Super Mario 3D World', 2013);&#xA;\"><tables><table name=\"mario_games\"><row><header content=\"name\" /><header content=\"release\" /></row><row><data content=\"Mario Bros.\" /><data content=\"1983\" /></row><row><data content=\"Mario Clash\" /><data content=\"1995\" /></row><row><data content=\"Super Mario Galaxy\" /><data content=\"2007\" /></row><row><data content=\"Super Mario 3D World\" /><data content=\"2013\" /></row></table></tables></database><code version=\"2\" codelang=\"sql\" name=\"script.sql\" visible-if=\"always\"><content><text value=\"SELECT\" class=\"keyword\" /><text value=\" \" /><text value=\"name\" class=\"keyword\" /><text value=\" \" /><text value=\"AS\" class=\"keyword\" /><text value=\" newest\" /><text type=\"break\" /><text value=\"FROM\" class=\"keyword\" /><text value=\" mario_games\" /><text type=\"break\" /><text value=\"WHERE\" class=\"keyword\" /><text value=\" \" /><text value=\"release\" class=\"keyword\" /><text value=\" = \" /><text value=\"2013\" class=\"number\" /><text value=\";\" /></content><interactions><interaction type=\"validatedInput\" isDefault=\"true\" startIndex=\"0\" endIndex=\"21\" /></interactions><output><table name=\"Result\"><row><header content=\"newest\" /></row><row><data content=\"Super Mario 3D World\" /></row></table></output></code><p visible-if=\"wrong\">Make sure to type <code>SELECT name AS newest</code>.</p></body></html>"
