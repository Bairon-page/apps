            "images": [],
            "interactiveContent": "<!DOCTYPE html ><html><body><p>What parameter do you use to include an API key?</p><selection iscode=\"false\" multiple=\"multiple\"><item value=\"1\" correct=\"true\">The <code>headers</code> parameter</item><item value=\"2\" correct=\"false\">The <code>body</code> parameter</item><item value=\"3\" correct=\"false\">The <code>params</code> parameter</item></selection><p visible-if=\"wrong\">The <code>headers</code> parameter.</p></body></html>"
