            "contentType": "interactive",
            "images": [],
            "interactiveContent": "<!DOCTYPE html ><html><body><p>What does <code>let</code> create?</p><selection iscode=\"false\" multiple=\"multiple\"><item value=\"1\" correct=\"true\">A variable</item><item value=\"2\" correct=\"false\">A continuous</item></selection><p visible-if=\"wrong\">A variable.</p></body></html>"
          },
