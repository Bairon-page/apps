          {
            "title": "Example: Aliasing",
            "id": 96789,
            "contentType": "interactive",
            "images": [],
            "interactiveContent": "<!DOCTYPE html ><html><body><p>Select <code>score</code> values greater than <code>7</code> under the alias <code>highest_score</code>.</p><database visible-if=\"always\" bootstrapScript=\"CREATE TABLE students (&#xA; name varchar(255),&#xA; score int&#xA;);&#xA;&#xA;INSERT INTO students (name, score)&#xA;VALUES ('Jean Miller', 5);&#xA;&#xA;INSERT INTO students (name, score)&#xA;VALUES ('Emma Brown', 8);&#xA;&#xA;INSERT INTO students (name, score)&#xA;VALUES ('Reese Jones', 2);&#xA;&#xA;INSERT INTO students (name, score)&#xA;VALUES ('Kim Lee', 9);&#xA;\"><tables><table name=\"students\"><row><header content=\"name\" /><header content=\"score\" /></row><row><data content=\"Jean Miller\" /><data content=\"5\" /></row><row><data content=\"Emma Brown\" /><data content=\"8\" /></row><row><data content=\"Reese Jones\" /><data content=\"2\" /></row><row><data content=\"Kim Lee\" /><data content=\"9\" /></row></table></tables></database><code version=\"2\" codelang=\"sql\" name=\"script.sql\" visible-if=\"always\"><content><text value=\"SELECT\" class=\"keyword\" /><text value=\" score \" /><text value=\"AS\" class=\"keyword\" /><text value=\" highest_score\" /><text type=\"break\" /><text value=\"FROM\" class=\"keyword\" /><text value=\" students\" /><text type=\"break\" /><text value=\"WHERE\" class=\"keyword\" /><text value=\" score &gt; \" /><text value=\"7\" class=\"number\" /><text value=\";\" /></content><interactions><interaction type=\"fillTheGap\" isDefault=\"true\"><options><option startIndex=\"0\" endIndex=\"12\" /><option startIndex=\"13\" endIndex=\"29\" /><option correct=\"false\"><content><text value=\"AS score\" /></content></option></options></interaction></interactions><output><table name=\"Result\"><row><header content=\"highest_score\" /></row><row><data content=\"8\" /></row><row><data content=\"9\" /></row></table></output></code><p visible-if=\"wrong\">Make sure to pick <code>SELECT score</code>, followed by <code>AS highest_score</code>.</p></body></html>"
          },
          {
            "title": "Example: AS",
            "id": 96790,
            "contentType": "interactive",
            "images": [],
            "interactiveContent": "<!DOCTYPE html ><html><body><p>Code <code>AS</code> to provide an alias for the result column.</p><database visible-if=\"always\" bootstrapScript=\"CREATE TABLE students (&#xA; name varchar(255),&#xA; score int&#xA;);&#xA;&#xA;INSERT INTO students (name, score)&#xA;VALUES ('Jean Miller', 5);&#xA;&#xA;INSERT INTO students (name, score)&#xA;VALUES ('Emma Brown', 8);&#xA;&#xA;INSERT INTO students (name, score)&#xA;VALUES ('Reese Jones', 2);&#xA;&#xA;INSERT INTO students (name, score)&#xA;VALUES ('Kim Lee', 9);&#xA;\"><tables><table name=\"students\"><row><header content=\"name\" /><header content=\"score\" /></row><row><data content=\"Jean Miller\" /><data content=\"5\" /></row><row><data content=\"Emma Brown\" /><data content=\"8\" /></row><row><data content=\"Reese Jones\" /><data content=\"2\" /></row><row><data content=\"Kim Lee\" /><data content=\"9\" /></row></table></tables></database><code version=\"2\" codelang=\"sql\" name=\"script.sql\" visible-if=\"always\"><content><text value=\"SELECT\" class=\"keyword\" /><text value=\" score \" /><text value=\"AS\" class=\"keyword\" /><text value=\" highest_score\" /><text type=\"break\" /><text value=\"FROM\" class=\"keyword\" /><text value=\" students\" /><text type=\"break\" /><text value=\"WHERE\" class=\"keyword\" /><text value=\" score &gt; \" /><text value=\"7\" class=\"number\" /><text value=\";\" /></content><interactions><interaction type=\"validatedInput\" isDefault=\"true\" startIndex=\"13\" endIndex=\"15\" /></interactions><output><table name=\"Result\"><row><header content=\"highest_score\" /></row><row><data content=\"8\" /></row><row><data content=\"9\" /></row></table></output></code><p visible-if=\"wrong\">Make sure to type <code>AS</code>.</p></body></html>"
          },
