            "images": [],
            "interactiveContent": "<!DOCTYPE html ><html><body><p>What does this code display in the console?</p><code version=\"2\" codelang=\"javascript\" name=\"script.js\" visible-if=\"always\"><content><text value=\"console\" class=\"built_in\" /><text value=\".log(\" /><text value=\"10\" class=\"number\" /><text value=\" + \" /><text value=\"&quot; years old&quot;\" class=\"string\" /><text value=\");\" /></content><output><content><text value=\"10 years old\" /></content></output></code><selection iscode=\"false\" multiple=\"multiple\"><item value=\"1\" correct=\"true\"><code>10 years old</code></item><item value=\"2\" correct=\"false\"><code>10 \"years\" old</code></item></selection><p visible-if=\"wrong\"><code>10 years old</code>.</p></body></html>"
