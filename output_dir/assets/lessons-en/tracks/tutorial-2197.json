            "images": [],
            "interactiveContent": "<!DOCTYPE html ><html><body><p>How many paragraphs will this webpage have?</p><code version=\"2\" codelang=\"html\" name=\"index.html\" visible-if=\"always\"><content><text value=\"&lt;\" /><text value=\"h1\" class=\"name\" /><text value=\"&gt;\" /><text value=\"My favorite things\" /><text value=\"&lt;/\" /><text value=\"h1\" class=\"name\" /><text value=\"&gt;\" /><text type=\"break\" /><text value=\"&lt;\" /><text value=\"p\" class=\"name\" /><text value=\"&gt;\" /><text value=\"Raindrops on roses Whiskers on kittens\" /><text value=\"&lt;/\" /><text value=\"p\" class=\"name\" /><text value=\"&gt;\" /><text type=\"break\" /><text value=\"&lt;\" /><text value=\"p\" class=\"name\" /><text value=\"&gt;\" /><text value=\"Bright copper kettles Warm woolen mittens\" /><text value=\"&lt;/\" /><text value=\"p\" class=\"name\" /><text value=\"&gt;\" /></content></code><selection iscode=\"false\" multiple=\"multiple\"><item value=\"1\" correct=\"true\">2</item><item value=\"2\" correct=\"false\">1</item><item value=\"4\" correct=\"false\">4</item></selection><p visible-if=\"wrong\">Make sure to pick 2.</p></body></html>"
          },
