            "id": 91174,
            "contentType": "interactive",
            "images": [],
            "interactiveContent": "<!DOCTYPE html ><html><body><p>Código <code>SELECT</code>, seguido de <code>DISTINCT</code> y la columna <code>platform</code>, para seleccionar solo valores únicos de <code>platform</code>.</p><database visible-if=\"always\" bootstrapScript=\"CREATE TABLE games (&#xA; game varchar(255),&#xA; platform varchar(255),&#xA; year int&#xA;);&#xA;&#xA;INSERT INTO games (game, platform, year)&#xA;VALUES ('Super Mario', 'NES', 1985);&#xA;&#xA;INSERT INTO games (game, platform, year)&#xA;VALUES ('Mario Kart', 'Wii', 2008);&#xA;&#xA;INSERT INTO games (game, platform, year)&#xA;VALUES ('Just Dance', 'Wii', 2009);&#xA;&#xA;INSERT INTO games (game, platform, year)&#xA;VALUES ('FIFA 16', 'PS4', 2015);&#xA;\"><tables><table name=\"games\"><row><header content=\"game\" /><header content=\"platform\" /><header content=\"year\" /></row><row><data content=\"Super Mario\" /><data content=\"NES\" /><data content=\"1985\" /></row><row><data content=\"Mario Kart\" /><data content=\"Wii\" /><data content=\"2008\" /></row><row><data content=\"Just Dance\" /><data content=\"Wii\" /><data content=\"2009\" /></row><row><data content=\"FIFA 16\" /><data content=\"PS4\" /><data content=\"2015\" /></row></table></tables></database><code version=\"2\" codelang=\"sql\" name=\"script.sql\" visible-if=\"always\"><content><text value=\"SELECT\" class=\"keyword\" /><text value=\" \" /><text value=\"DISTINCT\" class=\"keyword\" /><text value=\" platform\" /><text type=\"break\" /><text value=\"FROM\" class=\"keyword\" /><text value=\" games;\" /></content><interactions><interaction type=\"validatedInput\" isDefault=\"true\" startIndex=\"0\" endIndex=\"24\" /></interactions><output><table name=\"Result\"><row><header content=\"platform\" /></row><row><data content=\"NES\" /></row><row><data content=\"Wii\" /></row><row><data content=\"PS4\" /></row></table></output></code><p visible-if=\"wrong\">Asegúrese de elegir <code>SELECT DISTINCT platform</code>.</p></body></html>