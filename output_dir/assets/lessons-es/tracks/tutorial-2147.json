            "contentType": "interactive",
            "images": [],
            "interactiveContent": "<!DOCTYPE html ><html><body><p>Aparte de agregar, también puedes eliminar una fila de una tabla existente basándote en una condición.</p><database visible-if=\"always\" bootstrapScript=\"CREATE TABLE orders( &#xA; name varchar(255), &#xA; id int, &#xA; price int &#xA;); &#xA; &#xA;INSERT INTO orders(name, id, price) &#xA;VALUES(&quot;Chocolate box&quot;, 1235, 14); &#xA; &#xA;INSERT INTO orders(name, id, price) &#xA;VALUES(&quot;Bath bombs&quot;, 2337, 9); &#xA; &#xA;INSERT INTO orders(name, id, price) &#xA;VALUES(&quot;Flower hamper&quot;, 7483, 21); &#xA; &#xA;INSERT INTO orders(name, id, price) &#xA;VALUES(&quot;Teddy bear&quot;, 6574, 13);\"><tables><table name=\"orders\"><row><header content=\"name\" /><header content=\"id\" /><header content=\"price\" /></row><row><data content=\"Chocolate box\" /><data content=\"1235\" /><data content=\"14\" /></row><row><data content=\"Bath bombs\" /><data content=\"2337\" /><data content=\"9\" /></row><row><data content=\"Flower hamper\" /><data content=\"7483\" /><data content=\"21\" /></row><row><data content=\"Teddy bear\" /><data content=\"6574\" /><data content=\"13\" /></row></table></tables></database><code version=\"2\" codelang=\"sql\" name=\"script.sql\" visible-if=\"always\"><content><text value=\"DELETE\" class=\"keyword\" /><text value=\" \" /><text value=\"FROM\" class=\"keyword\" /><text value=\" orders  \" /><text type=\"break\" /><text value=\"WHERE\" class=\"keyword\" /><text value=\" price &lt; \" /><text value=\"10\" class=\"number\" /><text value=\"; \" /><text type=\"break\" /><text value=\" \" /><text type=\"break\" /><text value=\"SELECT\" class=\"keyword\" /><text value=\" * \" /><text value=\"FROM\" class=\"keyword\" /><text value=\" orders;\" /></content><interactions><interaction type=\"fillTheGap\" isDefault=\"true\"><options><option startIndex=\"0\" endIndex=\"6\" /><option startIndex=\"7\" endIndex=\"11\" /><option startIndex=\"21\" endIndex=\"26\" /></option