            "id": 91749,
            "contentType": "interactive",
            "images": [],
            "interactiveContent": "<!DOCTYPE html ><html><body><p>Напиши условие, чтобы выбрать все элементы <code>companies</code> с <code>years_active</code>, не равным <code>3</code>.</p><database visible-if=\"always\" bootstrapScript=\"CREATE TABLE companies (&#xA;name varchar(255),&#xA;years_active int&#xA;);&#xA;&#xA;INSERT INTO companies (name, years_active)&#xA;VALUES ('Rocket Delivery', 3);&#xA;INSERT INTO companies (name, years_active)&#xA;VALUES ('Teachya', 1);&#xA;INSERT INTO companies (name, years_active)&#xA;VALUES ('Hyped', 8);&#xA;INSERT INTO companies (name, years_active)&#xA;VALUES ('Thread Co.', 1);\"><tables><table name=\"companies\"><row><header content=\"name\" /><header content=\"years_active\" /></row><row><data content=\"Rocket Delivery\" /><data content=\"3\" /></row><row><data content=\"Teachya\" /><data content=\"1\" /></row><row><data content=\"Hyped\" /><data content=\"8\" /></row><row><data content=\"Thread Co.\" /><data content=\"1\" /></row></table></tables></database><code version=\"2\" codelang=\"sql\" name=\"script.sql\" visible-if=\"always\"><content><text value=\"SELECT\" class=\"keyword\" /><text value=\" *\" /><text type=\"break\" /><text value=\"FROM\" class=\"keyword\" /><text value=\" companies\" /><text type=\"break\" /><text value=\"WHERE\" class=\"keyword\" /><text value=\" years_active &lt;&gt; \" /><text value=\"3\" class=\"number\" /><text value=\";\" /></content><interactions><interaction type=\"fillTheGap\" isDefault=\"true\"><options><option startIndex=\"24\" endIndex=\"29\" /><option correct=\"false\"><content><text value=\"years_active = 3\" /></content></option><option startIndex=\"30\" endIndex=\"47\" /></options></interaction></interactions><output><table name=\"Result\"><row><header content=\"name\" /><header content=\"years_active\" /></row><row><data content=\"Teachya\" /><data content=\"1\" /></row><row><data content=\"Hyped\" /><data content=\"8\" /></row><row><data content=\"Thread Co.\" /><data content=