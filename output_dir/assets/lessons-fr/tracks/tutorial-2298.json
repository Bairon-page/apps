            "interactiveContent": "<!DOCTYPE html ><html><body><p>Utilise un alias pour renommer <code>COUNT(*)</code> en <code>items</code>.</p><database visible-if=\"always\" bootstrapScript=\"CREATE TABLE wishlist (&#xA; item varchar(255),&#xA; price numeric(2,1),&#xA; category varchar(255)&#xA;);&#xA;&#xA;INSERT INTO wishlist (item, price, category)&#xA;VALUES ('headphones', 250, 'electronics');&#xA;&#xA;INSERT INTO wishlist (item, price, category)&#xA;VALUES ('pencils', 37, 'stationery');&#xA;&#xA;INSERT INTO wishlist (item, price, category)&#xA;VALUES ('phone charger', 110, 'electronics');&#xA;&#xA;INSERT INTO wishlist (item, price, category)&#xA;VALUES ('daily planner', 29, 'stationery');&#xA;&#xA;\"><tables><table name=\"wishlist\"><row><header content=\"item\" /><header content=\"price\" /><header content=\"category\" /></row><row><data content=\"headphones\" /><data content=\"250\" /><data content=\"electronics\" /></row><row><data content=\"pencils\" /><data content=\"37\" /><data content=\"stationery\" /></row><row><data content=\"phone charger\" /><data content=\"110\" /><data content=\"electronics\" /></row><row><data content=\"daily planner\" /><data content=\"29\" /><data content=\"stationery\" /></row></table></tables></database><code version=\"2\" codelang=\"sql\" name=\"script.sql\" visible-if=\"always\"><content><text value=\"SELECT\" class=\"keyword\" /><text value=\" \" /><text value=\"category\" class=\"keyword\" /><text value=\", \" /><text value=\"COUNT\" class=\"keyword\" /><text value=\"(*) \" /><text value=\"AS\" class=\"keyword\" /><text value=\" items\" /><text type=\"break\" /><text value=\"FROM\" class=\"keyword\" /><text value=\" wishlist\" /><text type=\"break\" /><text value=\"GROUP\" class=\"keyword\" /><text value=\" \" /><text value=\"BY\" class=\"keyword\" /><text value=\" \" /><text value=\"category\" class=\"keyword\" /><text value=\";\" /></content><interactions><interaction type=\"validatedInput\" isDefault=\"true\" startIn