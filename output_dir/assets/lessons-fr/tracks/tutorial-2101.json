a content=\"Processing\" /><data content=\"54\" /><data content=\"3012\" /><data content=\"Store\" /><data content=\"\" /></row><row><data content=\"566\" /><data content=\"Delivered\" /><data content=\"850\" /><data content=\"3120\" /><data content=\"Store\" /><data content=\"\" /></row><row><data content=\"432\" /><data content=\"Shipped\" /><data content=\"99\" /><data content=\"3003\" /><data content=\"Amazon\" /><data content=\"\" /></row></table></tables></database><code version=\"2\" codelang=\"sql\" name=\"script.sql\" visible-if=\"always\"><content><text value=\"ALTER\" class=\"keyword\" /><text value=\" \" /><text value=\"TABLE\" class=\"keyword\" /><text value=\" orders \" /><text type=\"break\" /><text value=\"DROP\" class=\"keyword\" /><text value=\" \" /><text value=\"COLUMN\" class=\"keyword\" /><text value=\" place_of_order; \" /><text type=\"break\" /><text value=\" \" /><text type=\"break\" /><text value=\"SELECT\" class=\"keyword\" /><text value=\" * \" /><text value=\"FROM\" class=\"keyword\" /><text value=\" orders;\" /></content><interactions><interaction type=\"fillTheGap\" isDefault=\"true\"><options><option startIndex=\"20\" endIndex=\"24\" /><option startIndex=\"25\" endIndex=\"31\" /><option startIndex=\"32\" endIndex=\"46\" /></options></interaction></interactions><output><table name=\"Result\"><row><header content=\"order_no\" /><header content=\"order_status\" /><header content=\"order_amt\" /><header content=\"customer_id\" /><header content=\"discount\" /></row><row><data content=\"101\" /><data content=\"Delivered\" /><data content=\"550\" /><data content=\"3001\" /><data content=\"\" /></row><row><data content=\"512\" /><data content=\"Delivered\" /><data content=\"113\" /><data content=\"3001\" /><data content=\"\" /></row><row><data content=\"984\" /><data content=\"Processing\" /><data content=\"54\" /><data content=\"3012\" /><data content=\"\" /></row><row><data content=\"566\" /><data content=\"Delivered\" /><data content=\"85