            "title": "RENAME",
            "id": 118771,
            "contentType": "interactive",
            "images": [],
            "interactiveContent": "<!DOCTYPE html ><html><body><p>Tu peux renommer une colonne avec la commande <code>RENAME TO</code>. Ajoute le nom original de la colonne après <code>RENAME</code>, et ajoute le nouveau nom après le <code>TO</code>.</p><database visible-if=\"always\" bootstrapScript=\"CREATE TABLE orders( &#xA; order_no int, &#xA; order_status varchar(255), &#xA; order_amt int, &#xA; customer_id int, &#xA; place_of_order varchar(255) &#xA;); &#xA; &#xA;INSERT INTO orders(order_no, order_status, order_amt, customer_id, place_of_order) &#xA;VALUES(101, &quot;Delivered&quot;, 550, 3001, &quot;Amazon&quot;); &#xA; &#xA;INSERT INTO orders(order_no, order_status, order_amt, customer_id, place_of_order) &#xA;VALUES(512, &quot;Delivered&quot;, 113, 3001, &quot;Amazon&quot;); &#xA; &#xA;INSERT INTO orders(order_no, order_status, order_amt, customer_id, place_of_order) &#xA;VALUES(984, &quot;Processing&quot;, 54, 3012, &quot;Store&quot;); &#xA; &#xA;INSERT INTO orders(order_no, order_status, order_amt, customer_id, place_of_order) &#xA;VALUES(566, &quot;Delivered&quot;, 850, 3120, &quot;Store&quot;); &#xA; &#xA;INSERT INTO orders(order_no, order_status, order_amt, customer_id, place_of_order) &#xA;VALUES(432, &quot;Shipped&quot;, 99, 3003, &quot;Amazon&quot;); &#xA; &#xA;ALTER TABLE orders &#xA;ADD discount int; &#xA; &#xA;ALTER TABLE orders &#xA;DROP COLUMN place_of_order;\"><tables><table name=\"orders\"><row><header content=\"order_no\" /><header content=\"order_status\" /><header content=\"order_amt\" /><header content=\"customer_id\" /><header content=\"discount\" /></row><row><data content=\"101\" /><data content=\"Delivered\" /><data content=\"550\" /><data content=\"3001\" /><data content=\"\" /></row><row><data content=\"512\" /><data content=\"Delivered\" /><data content=\"113\" /><data content=\"3001\" /><data content=\"\" /></row><row><data content=\"984\" /><data content=\"Processing\" /><data content=\"54\" /><data content=\"3012\" /><data cont