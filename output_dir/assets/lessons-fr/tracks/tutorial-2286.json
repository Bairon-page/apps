            "images": [],
            "interactiveContent": "<!DOCTYPE html ><html><body><p>Code <code>athlete</code> et <code>time</code> séparés par une virgule <code>,</code> pour obtenir un tableau de tous les athlètes et le temps qu'ils ont mis pour terminer la course.</p><database visible-if=\"always\" bootstrapScript=\"CREATE TABLE race (&#xA; athlete varchar(255),&#xA; country varchar(255),&#xA; time varchar(255)&#xA;);&#xA;&#xA;INSERT INTO race (athlete, country, time)&#xA;VALUES ('Vivian Cheruiyot', 'Kenya', '14:26');&#xA;&#xA;INSERT INTO race (athlete, country, time)&#xA;VALUES ('Hellen Onsando Obiri', 'Kenya', '14:29');&#xA;&#xA;INSERT INTO race (athlete, country, time)&#xA;VALUES ('Almaz Ayana', 'Ethiopia', '14:33');&#xA;\"><tables><table name=\"race\"><row><header content=\"athlete\" /><header content=\"country\" /><header content=\"time\" /></row><row><data content=\"Vivian Cheruiyot\" /><data content=\"Kenya\" /><data content=\"14:26\" /></row><row><data content=\"Hellen Onsando Obiri\" /><data content=\"Kenya\" /><data content=\"14:29\" /></row><row><data content=\"Almaz Ayana\" /><data content=\"Ethiopia\" /><data content=\"14:33\" /></row></table></tables></database><code version=\"2\" codelang=\"sql\" name=\"script.sql\" visible-if=\"always\"><content><text value=\"SELECT\" class=\"keyword\" /><text value=\" athlete, \" /><text value=\"time\" class=\"built_in\" /><text type=\"break\" /><text value=\"FROM\" class=\"keyword\" /><text value=\" race;\" /></content><interactions><interaction type=\"validatedInput\" isDefault=\"true\" startIndex=\"7\" endIndex=\"20\" /></interactions><output><table name=\"Result\"><row><header content=\"athlete\" /><header content=\"time\" /></row><row><data content=\"Vivian Cheruiyot\" /><data content=\"14:26\" /></row><row><data content=\"Hellen Onsando Obiri\" /><data content=\"14:29\" /></row><row><data content=\"Almaz Ayana\" /><data content=\"14:33\" /></row></table></output></code><p visible-if=\"wrong\">Assurez-vous de rentrer <code>athlete, time</code>.</p></body></html>"
