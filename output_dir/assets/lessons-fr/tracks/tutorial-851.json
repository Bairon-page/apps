            "contentType": "interactive",
            "images": [],
            "interactiveContent": "<!DOCTYPE html ><html><body><p>Comment appelons-nous les fonctions à l'intérieur d'un objet ?</p><selection iscode=\"false\" multiple=\"multiple\"><item value=\"1\" correct=\"true\">Les méthodes de l'objet</item><item value=\"2\" correct=\"false\">Les fonctions de l'objet</item></selection><p visible-if=\"wrong\">Les méthodes de l'objet.</p></body></html>"
          },
