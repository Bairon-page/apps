            "interactiveContent": "<!DOCTYPE html ><html><body><p>Ändere den Wert in der <code>temperature</code>-Variable auf <code>\"100 degrees\"</code>.</p><code version=\"2\" codelang=\"swift\" name=\"main.swift\" visible-if=\"always\"><content><text value=\"var\" class=\"keyword\" /><text value=\" temperature = \" /><text value=\"&quot;0 degrees&quot;\" class=\"string\" /><text type=\"break\" /><text value=\"temperature = \" /><text value=\"&quot;100 degrees&quot;\" class=\"string\" /><text type=\"break\" /><text value=\"print\" class=\"built_in\" /><text value=\"(temperature)\" /></content><interactions><interaction type=\"fillTheGap\" isDefault=\"true\"><options><option correct=\"false\"><content><text value=\"var temperature\" /></content></option><option startIndex=\"30\" endIndex=\"41\" /></options></interaction></interactions><output><content><text value=\"100 degrees\" /></content></output></code><p visible-if=\"wrong\">Wähle <code>temperature</code> aus.</p></body></html>"
