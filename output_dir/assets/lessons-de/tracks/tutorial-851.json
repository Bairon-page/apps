            "interactiveContent": "<!DOCTYPE html ><html><body><p>Können Methoden Parameter und <code>return</code>-Anweisungen verwenden?</p><selection iscode=\"false\" multiple=\"multiple\"><item value=\"1\" correct=\"true\">Ja</item><item value=\"2\" correct=\"false\">Nein</item></selection><p visible-if=\"wrong\">Wähle Ja aus.</p></body></html>"
