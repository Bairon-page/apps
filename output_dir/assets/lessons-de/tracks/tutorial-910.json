            "contentType": "interactive",
            "images": [],
            "interactiveContent": "<!DOCTYPE html ><html><body><p>Wofür steht <code>const</code>?</p><selection iscode=\"false\" multiple=\"multiple\"><item value=\"1\" correct=\"true\">Konstante</item><item value=\"2\" correct=\"false\">kontinuierlich</item></selection><p visible-if=\"wrong\">Wähle Konstante aus.</p></body></html>"
          },
