            "images": [],
            "interactiveContent": "<!DOCTYPE html ><html><body><p>Welchen Typ hat <code>message</code>?</p><code version=\"2\" codelang=\"swift\" name=\"main.swift\" visible-if=\"always\"><content><text value=\"let\" class=\"keyword\" /><text value=\" isBooked = \" /><text value=\"true\" class=\"literal\" /><text type=\"break\" /><text value=\"let\" class=\"keyword\" /><text value=\" message = \" /><text value=\"&quot;Ticket booked: \\(isBooked)&quot;\" class=\"string\" /><text type=\"break\" /><text value=\"print\" class=\"built_in\" /><text value=\"(message)\" /></content><output><content><text value=\"Ticket booked: true\" /></content></output></code><selection iscode=\"false\" multiple=\"multiple\"><item value=\"1\" correct=\"true\">String</item><item value=\"2\" correct=\"false\">Boolescher Wert</item><item value=\"3\" correct=\"false\">Zahl</item></selection><p visible-if=\"wrong\">Wähle String aus.</p></body></html>"
