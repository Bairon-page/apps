            "interactiveContent": "<!DOCTYPE html ><html><body><p>Ändere den Wert der <code>currentStatus</code> Variable zu <code>\"Done\"</code>.</p><code version=\"2\" codelang=\"javascript\" name=\"script.js\" visible-if=\"always\"><content><text value=\"let\" class=\"keyword\" /><text value=\" currentStatus = \" /><text value=\"&quot;Working hard&quot;\" class=\"string\" /><text value=\";\" /><text type=\"break\" /><text value=\"currentStatus = \" /><text value=\"&quot;Done&quot;\" class=\"string\" /><text value=\";\" /><text type=\"break\" /><text value=\"console\" class=\"built_in\" /><text value=\".log(currentStatus);\" /><text type=\"break\" /></content><interactions><interaction type=\"validatedInput\" isDefault=\"true\" startIndex=\"52\" endIndex=\"58\" /></interactions><output><content><text value=\"Done\" /></content></output></code><p visible-if=\"wrong\">Gib <code>\"Done\"</code> ein.</p></body></html>"
