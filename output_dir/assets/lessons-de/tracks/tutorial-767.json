            "interactiveContent": "<!DOCTYPE html ><html><body><p>Was ist das Symbol zum Trennen von Werten in einem Array?</p><selection iscode=\"false\" multiple=\"multiple\"><item value=\"1\" correct=\"true\">Ein Komma <code>,</code></item><item value=\"2\" correct=\"false\">Ein Semikolon <code>;</code></item></selection><p visible-if=\"wrong\">Ein Komma <code>,</code>.</p></body></html>"
