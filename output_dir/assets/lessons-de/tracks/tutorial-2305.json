            "contentType": "executableFiles",
            "images": [],
            "executableContent": {
              "instructions": "Wähle die Namen der Patienten aus, die über dem Durchschnittsalter liegen, indem du eine Unterabfrage verwendest, die `AVG(age)` aus den Terminen auswählt. Wähle dann in der Hauptabfrage nur Einträge mit einem Alter über diesem Durchschnitt aus.",
              "preselectedFileIndex": 0,
              "hasVisualOutput": false,
              "files": [
                {
                  "name": "script.sql",
                  "codeLanguage": "sql",
                  "content": "SELECT patient_name\nFROM appointments\n",
                  "solvedContent": "SELECT patient_name\nFROM appointments\nWHERE age > (SELECT AVG(age) FROM appointments);"
                }
              ],
