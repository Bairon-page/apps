              "solutionDatabaseTables": "{\"Tables\":[{\"Name\":\"Result\",\"Rows\":[{\"Cells\":[{\"CellType\":\"Header\",\"Content\":\"id\"},{\"CellType\":\"Header\",\"Content\":\"name\"},{\"CellType\":\"Header\",\"Content\":\"type\"},{\"CellType\":\"Header\",\"Content\":\"id\"},{\"CellType\":\"Header\",\"Content\":\"carbs_g\"},{\"CellType\":\"Header\",\"Content\":\"protein_g\"},{\"CellType\":\"Header\",\"Content\":\"fat_g\"}]},{\"Cells\":[{\"CellType\":\"Data\",\"Content\":\"001\"},{\"CellType\":\"Data\",\"Content\":\"apple\"},{\"CellType\":\"Data\",\"Content\":\"vegan\"},{\"CellType\":\"Data\",\"Content\":\"001\"},{\"CellType\":\"Data\",\"Content\":\"14\"},{\"CellType\":\"Data\",\"Content\":\"0.2\"},{\"CellType\":\"Data\",\"Content\":\"0.2\"}]},{\"Cells\":[{\"CellType\":\"Data\",\"Content\":\"133\"},{\"CellType\":\"Data\",\"Content\":\"Cheerios\"},{\"CellType\":\"Data\",\"Content\":\"vegan\"},{\"CellType\":\"Data\",\"Content\":\"\"},{\"CellType\":\"Data\",\"Content\":\"\"},{\"CellType\":\"Data\",\"Content\":\"\"},{\"CellType\":\"Data\",\"Content\":\"\"}]},{\"Cells\":[{\"CellType\":\"Data\",\"Content\":\"455\"},{\"CellType\":\"Data\",\"Content\":\"chocolate\"},{\"CellType\":\"Data\",\"Content\":\"vegetarian\"},{\"CellType\":\"Data\",\"Content\":\"455\"},{\"CellType\":\"Data\",\"Content\":\"2\"},{\"CellType\":\"Data\",\"Content\":\"15\"},{\"CellType\":\"Data\",\"Content\":\"12\"}]},{\"Cells\":[{\"CellType\":\"Data\",\"Content\":\"060\"},{\"CellType\":\"Data\",\"Content\":\"cashews\"},{\"CellType\":\"Data\",\"Content\":\"vegan\"},{\"CellType\":\"Data\",\"Content\":\"\"},{\"CellType\":\"Data\",\"Content\":\"\"},{\"CellType\":\"Data\",\"Content\":\"\"},{\"CellType\":\"Data\",\"Content\":\"\"}]}]}]}"
